{
  "name": "pet-adoption-frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "Pet Adoption Platform Frontend - Vue.js Application",
  "author": "Pet Adoption Development Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "vuetify": "^3.4.0",
    "@mdi/font": "^7.3.67",
    "axios": "^1.6.0",
    "socket.io-client": "^4.7.4",
    "dayjs": "^1.11.10",
    "vee-validate": "^4.12.0",
    "yup": "^1.3.3",
    "@vueuse/core": "^10.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.18.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}